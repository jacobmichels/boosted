{
  "name": "boosted-app",
  "productName": "boosted-app",
  "version": "1.2.1",
  "description": "League Queue notifications",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Jacob",
    "email": "jacob.michels2025@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobmichels/boosted"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {"asar":true},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "boosted_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "jacobmichels",
              "name": "boosted"
            },
            "prerelease": true
          }
        }
      ]
    }
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "custom-electron-titlebar": "^3.2.2-hotfix62",
    "edit-json-file": "^1.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.5.0",
    "js-base64": "^2.5.2",
    "lcu-connector": "^2.1.3",
    "postman-request": "^2.88.1-postman.22",
    "update-electron-app": "^1.5.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "@electron-forge/publisher-github": "^6.0.0-beta.50",
    "electron": "8.2.0"
  }
}
